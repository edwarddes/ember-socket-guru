{
  "name": "ember-socket-guru",
  "version": "1.0.1",
  "description": "Addon for easy integration of WebSockets implementation in your ember app (pusher, socket.io, phoenix channels, Action Cable)",
  "keywords": [
    "ember-addon",
    "web sockets",
    "pusher",
    "socket.io",
    "action cable",
    "phoenix channels",
    "pusher-wrapper",
    "ember-pusher",
    "ember-socketio",
    "ember-phoenix-channels",
    "ember-actioncable",
    "ember"
  ],
  "repository": "https://github.com/netguru/ember-socket-guru",
  "license": "MIT",
  "author": {
    "name": "Jacek Bandura",
    "email": "jacek.bandura@netguru.pl"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "ember github-pages:commit -m \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push origin gh-pages:gh-pages; git checkout master"
  },
  "dependencies": {
    "broccoli-babel-transpiler": "^7.6.0",
    "ember-cli-babel": "^7.1.4",
    "ember-lodash": "4.17.4"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "^3.1.0",
    "ember-cli": "~3.5.1",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-github-pages": "0.1.2",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-node-assets": "0.2.2",
    "ember-cli-sass": "6.1.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-code-snippet": "1.8.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-route-action-helper": "2.0.2",
    "ember-sinon": "0.6.0",
    "ember-source": "~3.5.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-svg-jar": "0.9.3",
    "ember-try": "^1.0.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-config-netguru-ember": "^2.0.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-netguru-ember": "^1.6.6",
    "eslint-plugin-node": "^7.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
